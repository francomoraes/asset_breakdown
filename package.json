{
  "name": "backend",
  "version": "1.0.0",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ts-node src/server.ts",
    "seed": "ts-node -r tsconfig-paths/register src/seed/seed.ts",
    "seed:dev": "NODE_ENV=development ts-node src/seed/seed.ts",
    "seed:demo": "NODE_ENV=demo ts-node src/seed/seed.ts",
    "reset": "npm run seed -- --drop-schema --reset --clear-price-cache",
    "test:api": "newman run postman/collection.json -e postman/environment.json --reporters cli,json --reporter-json-export postman/results.json",
    "test:api:verbose": "newman run postman/collection.json -e postman/environment.json --reporters cli --verbose",
    "test:api:full": "newman run postman/asset_breakdown.postman_collection.json -e postman/environment.json --reporters cli",
    "test:api:auth": "newman run postman/asset_breakdown.postman_collection.json -e postman/environment.json --folder auth",
    "test:api:assets": "newman run postman/asset_breakdown.postman_collection.json -e postman/environment.json --folder assets",
    "test:api:html": "newman run postman/asset_breakdown.postman_collection.json -e postman/environment.json --reporters cli,html --reporter-html-export postman/report.html",
    "test:prod:auth": "newman run postman/asset_breakdown.postman_collection.json -e postman/production.environment.json --folder auth --reporters cli"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "fast-csv": "^5.0.2",
    "helmet": "^8.1.0",
    "json2csv": "^6.0.0-alpha.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.2",
    "pg": "^8.16.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.24",
    "winston": "^3.17.0",
    "yahoo-finance2": "^2.13.3",
    "zod": "^3.24.4",
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.1",
    "@types/express-rate-limit": "^5.1.3",
    "@types/helmet": "^0.0.48",
    "@types/json2csv": "^5.0.7",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.15.18",
    "@types/winston": "^2.4.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "newman": "^6.2.1",
    "nodemon": "^3.1.10",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.19.4"
  }
}
